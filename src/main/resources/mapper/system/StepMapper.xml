<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.mengweijin.quietly.system.mapper.StepMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.mengweijin.quietly.system.entity.Step">
    <result column="ID" property="id" />
    <result column="CREATE_BY" property="createBy" />
    <result column="CREATE_TIME" property="createTime" />
    <result column="UPDATE_BY" property="updateBy" />
    <result column="UPDATE_TIME" property="updateTime" />
    <result column="CASE_ID" property="caseId" />
    <result column="SEQ" property="seq" />
    <result column="ENABLED" property="enabled" />
    <result column="STEP_DEFINITION" property="stepDefinition" />
    <result column="ACTION_OR_ASSERT_KEY" property="actionOrAssertKey" />
    <result column="EXPECT_VALUE" property="expectValue" />
    <result column="ACTUAL_VALUE" property="actualValue" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID,
        CREATE_BY,
        CREATE_TIME,
        UPDATE_BY,
        UPDATE_TIME,
        CASE_ID, SEQ, ENABLED, STEP_DEFINITION, ACTION_OR_ASSERT_KEY, EXPECT_VALUE, ACTUAL_VALUE
    </sql>

</mapper>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.mengweijin.quietly.system.mapper.TestCaseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.mengweijin.quietly.system.entity.TestCase">
    <result column="ID" property="id" />
    <result column="CREATE_BY" property="createBy" />
    <result column="CREATE_TIME" property="createTime" />
    <result column="UPDATE_BY" property="updateBy" />
    <result column="UPDATE_TIME" property="updateTime" />
    <result column="CASE_NAME" property="caseName" />
    <result column="DESCRIPTION" property="description" />
    <result column="API_ID" property="apiId" />
    <result column="URL_ARGS" property="urlArgs" />
    <result column="REQUEST_ARGS" property="requestArgs" />
    <result column="STATUS" property="status" />
    <result column="FEATURE_CONTENT" property="featureContent" />
    <result column="EXCEPTION" property="exception" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID,
        CREATE_BY,
        CREATE_TIME,
        UPDATE_BY,
        UPDATE_TIME,
        CASE_NAME, DESCRIPTION, API_ID, URL_ARGS, REQUEST_ARGS, STATUS, FEATURE_CONTENT, EXCEPTION
    </sql>

</mapper>
